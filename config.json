{
  "name": "json",
  "viewName": "JSON",
  "type": "executer",
  "main": "app.js",
  "execProgram": "node",
  "exposeRoute": false,
  "imgUrl": "logo.png",
  "version": "1.0.0",
  "description": "Kaholo plugin used to process JSON files.",
  "category": [
    "Basic Tools"
  ],
  "keywords": [
    "json",
    "file",
    "files",
    "read",
    "write",
    "parse"
  ],
  "methods": [
    {
      "name": "readJsonFile",
      "viewName": "Read JSON file",
      "params": [
        {
          "name": "filePath",
          "viewName": "File path",
          "type": "string",
          "required": true,
          "description": "A JSON file to parse and deliver in Final Result"
        }
      ]
    }, {
      "name": "writeJsonFile",
      "viewName": "Write JSON file",
      "params": [
        {
          "name": "filePath",
          "viewName": "File path",
          "type": "string",
          "required": true,
          "description": "A path and file name where the JSON document will be written as a one-line string"
        },
        {
          "name": "json",
          "viewName": "JSON Document",
          "type": "text",
          "parserType": "object",
          "required": true,
          "description": "The JSON document as multiline text, one-line string or a code layer object"
        }
      ]
    }
  ]
}
